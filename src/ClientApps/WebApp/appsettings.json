{
  // Superseded by Serilog
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApiSettings": {
    "GatewayAddress": "http://localhost:8010"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "ElasticSearch": "http://localhost:9200",
    "Zipkin": "http://localhost:9411/api/v2/spans"
  }
}
