{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "DiscountGrpc": "http://localhost:5003",
    "RabbitMQ": "amqp://guest:guest@localhost:5672",
    "ElasticSearch": "http://localhost:9200",
    "Zipkin": "http://localhost:9411/api/v2/spans"
  },
  "IdentityServer": {
    "Authority": "http://localhost:5011"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  }
}
